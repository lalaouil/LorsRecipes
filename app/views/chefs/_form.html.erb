<%= render 'shared/errors', obj: @chef %>

<div class="row">
  <div class="well col-sm-4 col-sm-offset-4">
    <%= form_for @chef do |f| %>
      <%= f.label :name %>
      <%= f.text_field :chefname %>
      
      <%= f.label :email %>
      <%= f.email_field :email %>
      
      <%= f.label :password %>
      <%= f.password_field :password %>
      
      <%= f.label :password_confirmation, "Confirm Password" %>
      <%= f.password_field :password_confirmation %>
      
      <%= f.submit(@chef.new_record? ? 'Create my account' : 'Edit Profile', class:"btn-submit btn-success") %>
    <% end %>
  </div>
</div>