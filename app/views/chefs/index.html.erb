<%= render 'shared/page_title', title: 'All Contributers' %>
<div class="container">
<div class="tabbable-panel">
  <div class="tabbable-line">
    <ul class="nav nav-tabs">
      <li>
        <%= link_to "All Recipes", recipes_path %>
      </li>
      <li class="active">
        <%= link_to "All Contributers", chefs_path %>
      </li>
    </ul>
    <div class="tab-content">
		  <div class="tab-pane active" id="tab_default_2">
        <div class="row">
        </div>
      </div>
    </div>

<div class="container">
  <% @chefs.each do |chef| %>
  <div class= "col-sm-6 col-sm-6 center">
  <%= link_to gravatar_for(chef, size: 200), chef_path(chef) %>
    <p>
      <h5>Contributer: <%= chef.chefname %></h5>
    </p>
    <p>&nbsp</p>
  </div>
<% end %>
</div>
</div>
</div>
</div>

<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
    <%= will_paginate %>
    <p>&nbsp</p>
  </div>
</div>
