<%= render 'shared/page_title', title: 'Log in' %>

<div class= "row">
  <div class= 'well col-md-4 col-md-offset-4'>
    <%= form_tag '/login' do %>
    
      <%= label_tag :email %>
      <%= text_field_tag :email %>
      
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      
      <%= submit_tag 'Login', class: 'btn-submit btn-success' %>
      
    <% end %>
  </div>
</div>